# Temporal configuration
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default

# Server configuration
SERVER_URL=http://localhost:8080
AUTH_TOKEN=your_auth_token
CORS_HEADERS=Content-Type,Authorization
CORS_METHODS=GET,POST,OPTIONS
CORS_ORIGINS=*

# Solana configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_WS_URL=wss://api.devnet.solana.com
SOLANA_ESCROW_PRIVATE_KEY=your_private_key
SOLANA_ESCROW_TOKEN_ACCOUNT=your_token_account

# LLM configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4

# Reddit configuration
REDDIT_USER_AGENT=MyApp/1.0
REDDIT_USERNAME=your_reddit_username
REDDIT_PASSWORD=your_reddit_password
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# YouTube configuration
YOUTUBE_API_KEY=your_youtube_api_key
YOUTUBE_APP_NAME=MyYouTubeApp

# Yelp configuration
YELP_API_KEY=your_yelp_api_key
YELP_CLIENT_ID=your_yelp_client_id

# Google configuration
GOOGLE_API_KEY=your_google_api_key
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id

# Amazon configuration
AMAZON_API_KEY=your_amazon_api_key
AMAZON_API_SECRET=your_amazon_api_secret
AMAZON_ASSOCIATE_TAG=your_amazon_associate_tag
AMAZON_MARKETPLACE_ID=your_amazon_marketplace_id